{
  "name": "multigame_browser",
  "version": "0.5.1",
  "description": "Directory for streams of multiple games on Twitch.",
  "main": "src/server/server.js",
  "scripts": {
    "start": "babel-node src/server/server.js",
    "build": "webpack -p",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/server.js --exec babel-node",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "mocha",
    "test:min": "mocha -R mocha-fivemat-reporter",
    "test:watch": "mocha --watch",
    "heroku-postbuild": "webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ksnovak/Multigame_Browser"
  },
  "repo_root": "https://github.com/ksnovak/Multigame_Browser",
  "homepage": "https://github.com/ksnovak/Multigame_Browser#readme",
  "author": "Kevin Novak",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.4",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "query-string": "^6.1.0",
    "react-animate-on-change": "^2.1.1",
    "react-dom": "^16.5.2",
    "react-select": "^2.0.0",
    "react-spinkit": "^3.0.0",
    "react": "^16.5.2",
    "request": "^2.88.0",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9",
    "webpack": "^4.20.2"
  },
  "devDependencies": {
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.2.0",
    "chai": "^4.2.0",
    "concurrently": "^4.0.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint": "^5.6.0",
    "mocha-clean": "^1.0.0",
    "mocha-fivemat-reporter": "0.0.9",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "prettier-eslint": "^8.8.2"
  }
}
